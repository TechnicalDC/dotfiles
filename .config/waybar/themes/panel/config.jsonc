// -*- mode: jsonc -*-
{
   "layer": "top", // Waybar at top layer
   "position": "top", // Waybar position (top|bottom|left|right)
   "height": 20, // Waybar height (to be removed for auto height)
   // "width": 1884, // Waybar width
   "spacing": 0, // Gaps between modules (4px)
   "reload_style_on_change": true,
   "modules-left": [
      "custom/appmenuicon",
      "clock"
   ],
   "modules-center": [
      "hyprland/workspaces"
   ],
   "modules-right": [
      // "backlight/slider",
      // "pulseaudio/slider",
      "backlight",
      "pulseaudio",
      "custom/notifications",
      "power-profiles-daemon",
      "battery",
      "tray",
      "custom/powermenuicon"
   ],
   // Modules configuration
   "hyprland/workspaces": {
         "format": "{id}.{icon}",
         "format-icons": {
            "1": "dev",
            "2": "web",
            "3": "doc",
            "4": "img",
            "5": "vfx",
            "6": "sys",
            "7": "fun",
            "8": "chat",
            "9": "mics"
         },
         "all-outputs": true,
         "persistent-workspaces": {}
   },
   // Rofi Application Launcher
   "custom/appmenuicon": {
      "format": "ÓΩÜ",
      "on-click": "rofi -show drun",
      "tooltip": false,
      "tooltip-format": "<span color='#abb2bf'>Open the application launcher</span>"
   },
   "custom/powermenuicon": {
      "format": "ÔÄë",
      "on-click": "rofi-powermenu",
      "tooltip": false,
      "tooltip-format": "<span color='#abb2bf'>Open the power menu</span>"
   },
   "custom/notifications": {
      "format": "{icon}",
      "format-icons": {
         "notification": "<span foreground='#ea6962'>ÔÉ≥</span>",
         "none": "ÔÉ≥",
         "dnd-notification": "<span foreground='#ea6962'>Ôá∂</span>",
         "dnd-none": "Ôá∂",
         "inhibited-notification": "<span foreground='#ea6962'>ÔÉ≥</span>",
         "inhibited-none": "ÔÉ≥",
         "dnd-inhibited-notification": "<span foreground='#ea6962'>Ôá∂</span>",
         "dnd-inhibited-none": "Ôá∂"
      },
      "return-type": "json",
      "exec-if": "which swaync-client",
      "exec": "swaync-client -swb",
      "on-click": "swaync-client -t -sw",
      "on-click-right": "swaync-client -d -sw",
      "escape": true,
      "tooltip": false
   },
   "keyboard-state": {
      "numlock": true,
      "capslock": true,
      "format": "{name} {icon}",
      "format-icons": {
         "locked": "ÔÄ£",
         "unlocked": "ÔÇú"
      }
   },
   "mpd": {
      "format": "{stateIcon} {consumeIcon}{randomIcon}{repeatIcon} :: ÔÄÅ  {title} ({elapsedTime:%M:%S}/{totalTime:%M:%S}) :: {volume}%",
      "format-disconnected": "Disconnected ÔÄÅ",
      "format-stopped": "{consumeIcon}{randomIcon}{repeatIcon} :: ÔÄÅ  Stopped",
      "unknown-tag": "N/A",
      "title-len": 30,
      "interval": 5,
      "on-click": "rofi-mpc",
      "consume-icons": {
         "off": "<span color=\"#5b6078\">ÔÉÑ </span> ",
         "on": "<span color=\"#24273a\">ÔÉÑ </span> "
      },
      "random-icons": {
         "off": "<span color=\"#5b6078\">ÔÅ¥ </span> ",
         "on": "<span color=\"#24273a\">ÔÅ¥ </span> "
      },
      "repeat-icons": {
         "off": "<span color=\"#5b6078\">ÔÄû </span> ",
         "on": "<span color=\"#24273a\">ÔÄû </span> "
      },
      "single-icons": {
         "on": "ÔÄû 1",
         "off": "ÔÄû 1"
      },
      "state-icons": {
         "paused": "ÔÅå ",
         "playing": "ÔÅã "
      },
      "tooltip": false,
      "tooltip-format": "MPD (connected)",
      "tooltip-format-disconnected": "MPD (disconnected)"
   },
   "idle_inhibitor": {
      "format": "{icon}",
      "format-icons": {
         "activated": "ÔÅÆ",
         "deactivated": "ÔÅ∞"
      }
   },
   "tray": {
       // "icon-size": 21,
      "tooltip": false,
      "spacing": 10
   },
   "clock": {
       // "timezone": "America/New_York",
      "tooltip": false,
       "tooltip-format": "<tt>{calendar}</tt>",
       "format" : "{:%d %b | %H:%M}",
       "on-click": "ghostty -e calcurse &",
       "actions":  {
          "on-click-right": "mode",
          "on-click-forward": "tz_up",
          "on-click-backward": "tz_down",
          "on-scroll-up": "shift_up",
          "on-scroll-down": "shift_down"
       },
       "calendar": {
          "mode"          : "month",
          "mode-mon-col"  : 3,
          "weeks-pos"     : "left",
          "on-scroll"     : 1,
          "format": {
             "months":     "<span color='#7daea3'><b>{}</b></span>",
             "days":       "<span color='#d3869b'><b>{}</b></span>",
             "weeks":      "<span color='#89b482'><b>W{}</b></span>",
             "weekdays":   "<span color='#d8a657'><b>{}</b></span>",
             "today":      "<span color='#ea6962'><b>{}</b></span>"
          }
       }
   },
   "cpu": {
      "format": " Ôãõ  {usage}%",
      "tooltip": false
   },
   "memory": {
      "format": "ÓøÖ  {}%"
   },
   "temperature": {
       // "thermal-zone": 2,
       // "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
       "critical-threshold": 80,
       // "format-critical": "{temperatureC}¬∞C {icon}",
       "format": "{temperatureC}¬∞C {icon}",
       "format-icons": ["Ôù´", "Ôãâ", "Ôù©"]
   },
   "backlight": {
       // "device": "acpi_video1",
       "format": "{icon} {percent}%",
       "format-icons": ["Û±©è ", "Û±©ë ", "Û±©ì ", "Û±©ï "]
       // "format-icons": ["üåë", "üåò", "üåó", "üåñ", "üåï"]
   },
   "backlight/slider": {
      "min": 10,
      "max": 100,
      "orientation": "horizontal",
      "device": "intel_backlight"
   },
   "battery": {
      "states": {
           "good": 95,
           "warning": 30,
           "critical": 15
      },
      "tooltip": false,
      "format": "{icon}  {capacity}%",
      "format-full": "{icon} {capacity}%",
      "format-charging": "Ó∫°  {capacity}%",
      "format-plugged": "Ôá¶ {capacity}%",
      "format-alt": "{icon} {time}",
       // "format-good": "", // An empty format will hide the module
       "format-icons": [
          "<span color=\"#ea6962\">ÔâÑ </span>",
          "<span color=\"#ea6962\">ÔâÉ </span>",
          "<span color=\"#d8a657\">ÔâÇ </span>",
          "<span color=\"#a9b665\">ÔâÅ </span>",
          "<span color=\"#a9b665\">ÔâÄ </span>"
       ]
   },
   "battery#bat2": {
      "bat": "BAT2"
   },
   "power-profiles-daemon": {
      "format": "{icon}",
      "tooltip-format": "Power profile: {profile}\nDriver: {driver}",
      "tooltip": false,
      "format-icons": {
         "default": "<span color=\"#d8a657\">ÔÉß</span>",
         "performance": "<span color=\"#d8a657\">ÔÉß</span>",
         "balanced": "<span color=\"#7daea3\">Ôâé </span>",
         "power-saver": "<span color=\"#a9b665\">ÔÅ¨ </span>"
      }
   },
   "network": {
       // "interface": "wlp2*", // (Optional) To force the use of this interface
       "format-wifi": "Ôá´a {essid} ({signalStrength}%)",
       "format-ethernet": "{ipaddr}/{cidr} Ôûñ",
      "tooltip": false,
       "tooltip-format": "{ifname} via {gwaddr}",
       "format-linked": "ÔÉÅ  {ifname} (No IP)",
       "format-disconnected": "ÔÑß  Disconnected",
       "format-alt": "{ifname}: {ipaddr}/{cidr}"
   },
   "pulseaudio": {
       // "scroll-step": 1, // %, can be a float
       "format": "{icon} {volume}% {format_source}",
       "format-bluetooth": "{icon} {volume}% Ôäî {format_source}",
       "format-bluetooth-muted": "ÔÑ± {icon}Ôäî {format_source}",
       "format-muted": "Ôö© {format_source}",
       "format-source": "ÔÑ∞ {volume}%",
       "format-source-muted": "ÔÑ± ",
       "format-icons": {
          "headphone": "ÔÄ• ",
          "headset": "Óπô ",
          "default": ["ÔÄ¶ ", "ÔÄß ", "ÔÄ® "]
       },
       "on-click": "pavucontrol"
   },
   "pulseaudio/slider": {
      "min": 0,
      "max": 100,
      "orientation": "horizontal"
   },
   "user": {
      "format": "{user} (up {work_d} days ‚Üë)",
      "interval": 60,
      "height": 30,
      "width": 30,
      "icon": true
   },
   "wlr/taskbar": {
      "format": "{icon}",
      "icon-size": 20,
      "icon-theme": "Numix-Circle",
      "markup": true,
      "tooltip": false,
      "tooltip-format": "<span color='#abb2bf'>{title} ({state}</span>",
      "on-click": "activate",
      "on-click-middle": "close",
      "ignore-list": [
         "Alacritty"
      ],
      "app_ids-mapping": {
         "firefoxdeveloperedition": "firefox-developer-edition"
      },
      "rewrite": {
         "Firefox Web Browser": "Firefox",
         "Foot Server": "Terminal"
      }
   },
   "cava": {
      // "cava_config": "$XDG_CONFIG_HOME/cava/cava.conf",
      "framerate": 30,
      "autosens": 1,
      "sensitivity": 100,
      "bars": 14,
      "lower_cutoff_freq": 50,
      "higher_cutoff_freq": 10000,
      "method": "pulse",
      "source": "auto",
      "stereo": true,
      "reverse": false,
      "bar_delimiter": 0,
      "monstercat": false,
      "waves": false,
      "noise_reduction": 0.77,
      "input_delay": 2,
      "format-icons" : ["‚ñÅ", "‚ñÇ", "‚ñÉ", "‚ñÑ", "‚ñÖ", "‚ñÜ", "‚ñá", "‚ñà" ],
      "actions": {
         "on-click-right": "mode"
      }
   },
}
